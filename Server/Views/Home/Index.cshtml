<div id="projectid" data-name="@ViewData["projectid"]"></div>
<section id="postsContainer" >
    <div id="posts">
    </div>
</section>

<script id="postsTemplate" type="text/x-jQuery-tmpl">
    {{each postsList}}{{tmpl($value) "#postTemplate"}}{{/each}}
</script>

<script id="postTemplate" type="text/x-jQuery-tmpl">
    <div class="post">
        <a class="text" href="${UrlToPost}" target="_blank">${Text}
        </a>
        <div class="identity">
            <div class="avatar">
                <a href="${UrlToUserProfile}">
                    <img src="${UrlToUserAvatar}" />
                </a>
            </div>
            <div class="names">
                <a class="name" href="${UrlToUserProfile}" target="_blank">${Name}</a>
                <br />
                {{if SourceService === 'twitter'}}
                <a class="screenName" href="${UrlToUserProfile}" target="_blank">@@${ScreenName.toLowerCase()} (${FollowersCount})</a>
                <br />
                {{/if}}
                <a class="time" href="${UrlToPost}" target="_blank">${getPostTimeText(DateCreated)}</a>
            </div>
            <img class="sourceService" src="${(SourceService === 'twitter') ? '/images/twitter.png' : '/images/googleplus.png'}" />
        </div>
    </div>
</script>

